{
  "author": {
    "email": "bugs@udva.re",
    "name": "Andrew Udvare"
  },
  "bin": "./dist/itunes-clear-orphaned.js",
  "bugs": {
    "email": "bugs@udva.re",
    "url": "https://github.com/Tatsh/itunes-clear-orphaned/issues"
  },
  "description": "Remove orphaned tracks from iTunes/Music.app.",
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "cspell": "^8.19.2",
    "markdownlint-cli2": "^0.17.2",
    "prettier": "^3.5.3",
    "prettier-plugin-ini": "^1.3.0",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-toml": "^2.0.4",
    "typescript": "^5.8.3",
    "yarn-audit-fix": "^10.1.1"
  },
  "homepage": "https://github.com/Tatsh/itunes-clear-orphaned",
  "keywords": [
    "applescript",
    "itunes",
    "jxa",
    "music",
    "osascript"
  ],
  "license": "MIT",
  "markdownlint-cli2": {
    "config": {
      "MD033": {
        "allowed_elements": [
          "kbd"
        ]
      },
      "default": true,
      "line-length": {
        "code_blocks": false,
        "line_length": 100
      }
    }
  },
  "name": "itunes-clear-orphaned",
  "os": [
    "darwin"
  ],
  "packageManager": "yarn@4.9.1",
  "prettier": {
    "endOfLine": "lf",
    "iniSpaceAroundEquals": true,
    "jsonRecursiveSort": true,
    "overrides": [
      {
        "files": [
          "package.json"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "plugins": [
      "@prettier/plugin-xml",
      "prettier-plugin-ini",
      "prettier-plugin-sort-json",
      "prettier-plugin-toml"
    ],
    "printWidth": 100,
    "reorderKeys": true,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Tatsh/itunes-clear-orphaned.git"
  },
  "scripts": {
    "build": "yarn tsc && chmod +x ./dist/itunes-clear-orphaned.js",
    "run": "./dist/itunes-clear-orphaned.js"
  },
  "version": "0.0.3"
}
